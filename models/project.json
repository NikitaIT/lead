{
  "name": "models",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "models/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "models/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate dev --schema ./prisma/schema.prisma",
        "cwd": "models"
      }
    },
    "migrate-deploy": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db deploy --schema ./prisma/schema.prisma",
        "cwd": "models"
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma studio",
        "cwd": "models"
      }
    },
    "migrate-push": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db push --schema ./prisma/schema.prisma",
        "cwd": "models"
      }
    },
    "gen-client": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./prisma/schema.prisma",
        "cwd": "models"
      }
    }
  },
  "tags": []
}
